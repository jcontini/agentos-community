{
  "name": "agentos-integrations",
  "version": "0.1.0",
  "description": "Apps and connectors for AgentOS",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:schema": "vitest run --config vitest.schema.config.ts",
    "test:capabilities": "vitest run tests/adapters/operations.test.ts",
    "test:needs-work": "vitest run adapters/.needs-work",
    "validate": "node tests/adapters/scripts/validate.mjs",
    "validate:strict": "node tests/adapters/scripts/validate.mjs --no-move",
    "validate:patterns": "node tests/adapters/scripts/validate-patterns.mjs",
    "lint:tests": "npx tsx tests/adapters/scripts/lint-tests.ts",
    "new-adapter": "npx tsx tests/adapters/scripts/new-adapter.ts",
    "test:changed": "./tests/test-changed.sh --staged",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "husky": "^9.1.7",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "yaml": "^2.8.2"
  },
  "dependencies": {
    "js-yaml": "^4.1.1",
    "zod": "^4.3.5"
  }
}
